{
  "name": "statutory",
  "version": "1.9.1",
  "description": "Statutory events module for MyAEGEE",
  "main": "lib/server.js",
  "scripts": {
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "db:create": "sequelize db:create",
    "db:setup": "sequelize db:drop; sequelize db:create; sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "test": "NODE_ENV=test npm run db:setup && jest test/api/*.js --runInBand --forceExit",
    "test:ci": "NODE_ENV=test npm run db:setup && jest --runInBand --forceExit",
    "cli": "node --experimental-repl-await -r ./cli.js",
    "open-coverage": "open-cli coverage/lcov-report/index.html",
    "prepare": "husky install"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "models/**/*.js",
      "!lib/run.js",
      "!lib/sequelize.js",
      "!lib/logger.js",
      "!lib/fs.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "keywords": [],
  "author": "Sergey Peshkov <github.com/serge1peshcoff>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AEGEE/statutory/issues"
  },
  "homepage": "https://github.com/AEGEE/statutory",
  "dependencies": {
    "@bugsnag/js": "^7.13.3",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "express-query-boolean": "^2.0.0",
    "file-type": "^16.5.3",
    "joi": "^17.4.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-range": "^4.0.2",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "node-schedule": "^2.0.0",
    "node-xlsx": "^0.19.0",
    "on-finished": "^2.3.0",
    "pg": "^8.7.1",
    "prom-client": "^14.0.1",
    "read-chunk": "^3.2.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rimraf": "^3.0.2",
    "sequelize": "^6.9.0",
    "sequelize-cli": "^6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog": "^3.1.24",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "lint-staged": "^11.2.6",
    "nock": "^13.2.1",
    "nyc": "^15.1.0",
    "open-cli": "^7.0.1",
    "semantic-release": "^18.0.0",
    "timekeeper": "^2.2.0"
  }
}
